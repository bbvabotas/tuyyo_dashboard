{
  "family": "%TASK_FAMILY%",
  "networkMode": "bridge",
  "containerDefinitions": [{
    "memory": 990,
    "portMappings": [{
      "hostPort": 8080,
      "containerPort": 8080,
      "protocol": "tcp"
    }],
    "essential": true,
    "name": "tuyyo-dashboard",
    "environment": [],
    "workingDirectory": "/etc/tuyyo-dashboard",
    "image": "52.210.54.51:5000/%IMAGE_NAME%",
    "command": [
      "/bin/bash -c \"/usr/local/bin/npm install && /usr/local/bin/npm run dev\""
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "%AWS_LOG_GROUP%",
        "awslogs-region": "%AWS_REGION%",
        "awslogs-stream-prefix": "tuyyo-dashboard"
      }
    },
    "cpu": 0
  }]
}
